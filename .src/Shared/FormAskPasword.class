' Gambas class file


Public Sub ButtonOK_Click()
  FMain.password = TextBoxPassword.Text
  Me.Close
End

Public Sub TextBoxPassword_KeyPress()
  ' on press enter do like ok
  If Key.Code = Key.Return Then
    ButtonOK_Click()
  Endif
End

Public Sub ButtonCancel_Click()
  Me.Close
  Error.Raise("") ' cancel process of connecting to existing database and return back to home window
End
